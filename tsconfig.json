{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/Components/HeartButton.js", "app/Components/listings/ListingCard.js", "app/Components/listings/ListingHead.js", "app/Components/listings/ListingInfo.js", "app/Components/Navbar/Navbar.js", "app/Components/Navbar/UserMenus.js", "app/favourites/Favourites.js", "app/hooks/useFavourite.js", "app/listings/[listingId]/ListingClient.js", "app/properties/Properties.js", "app/reservations/Reservations.js", "app/trips/Trips.js", "app/favourites/page.js", "app/page.js", "app/listings/[listingId]/page.js"],
  "exclude": ["node_modules","app/types/index.ts"]
}
